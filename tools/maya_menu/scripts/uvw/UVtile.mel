global proc UVtile(){string $window = `window`;frameLayout -label "UV edit" -labelAlign "top"           -borderStyle "in";formLayout myForm ;	intField -w 32 -h 20 -maxValue 50 -minValue -50 -step 1 -value 1 "sizeUV" ;	symbolButton -i "sphere.xpm" -w 32 -h 32 -c "polyEditUV -v 1"  "up" ;	symbolButton -i "sphere.xpm" -w 32 -h 32 -c "polyEditUV -u -1" "left" ;	symbolButton -i "sphere.xpm" -w 32 -h 32 -c "polyEditUV -v -1"  "down" ;	symbolButton -i "sphere.xpm" -w 32 -h 32 -c "polyEditUV -u 1" "right" ;	intScrollBar -minValue -20 -maxValue 20 -value 1 -horizontal 1 -width 62 -height 20 "Scroll" ;	button -l "auto" -w 96 -h 20 -c "UVauto()"  "auto" ;	 	formLayout -e	-af "sizeUV" "left" 2	-af "sizeUV" "top" 0	-af "Scroll" "left" 37	-af "Scroll" "top" 0	-af "up" "left" 36	-af "up" "top" 32	-af "left" "left" 4	-af "left" "top" 64	-af "down" "left" 36	-af "down" "top" 64	-af "right" "left" 68	-af "right" "top" 64	-af "auto" "left" 4	-af "auto" "top" 106myForm ;    window -tlb true -te 30 -title "UV tile Edit" -s 0 -e -wh 116 178 $window;showWindow $window;}global proc UVauto(){$uvs = `ls-sl-fl`;for($uv in $uvs){$uvc=`polyEditUV -q $uv`;polyEditUV -r false -u  (($uvc[0]%1.0)) -v  (($uvc[1]%1.0)) $uv;};}