global proc string[] conv(string $itemsForConv[], string $mode){$itemsForConv = `ls -fl $itemsForConv`;string $converted[] = {};if ($mode == "v")	$converted = `polyListComponentConversion -tv $itemsForConv`;else if ($mode == "e")	$converted = `polyListComponentConversion -te $itemsForConv`;else if ($mode == "f")	$converted = `polyListComponentConversion -tf $itemsForConv`;else if ($mode == "u")	$converted = `polyListComponentConversion -tuv $itemsForConv`;else if ($mode == "vf")	$converted = `polyListComponentConversion -tvf $itemsForConv`;else if ($mode == "vi")	$converted = `polyListComponentConversion -tv -in $itemsForConv`;else if ($mode == "ei")	$converted = `polyListComponentConversion -te -in $itemsForConv`;else if ($mode == "fi")	$converted = `polyListComponentConversion -tf -in $itemsForConv`;else if ($mode == "ui")	$converted = `polyListComponentConversion -tuv -in $itemsForConv`;else if ($mode == "vfi")	$converted = `polyListComponentConversion -tvf -in $itemsForConv`;else	print "wrong argument";$converted = `ls -fl $converted`;return $converted;}