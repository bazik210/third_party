global proc int nearestPointOnMeshCreate (string $objects[]){//$objects = sel();//nearestPointOnMeshCreate(selObjs());string $object;int $count = 0;for ($object in $objects)	{//$object = $objects[0];	if (!`objExists ("nearestPointOnMesh_" + $object)`)		{		nearestPointOnMesh -na ("nearestPointOnMesh_" + $object) $object;		$count++;		}	}//print ($count + " nodes created from " + `size $objects` + " objects specified.");return $count;}