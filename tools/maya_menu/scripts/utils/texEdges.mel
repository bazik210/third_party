global proc string[] texEdges(string $items[]){//$items = `ls -sl`;$edges = conv($items, "e");int $showProgress = 0;if (`size $edges` > 500)	{	$showProgress = 1;	progressBegin ("tex edges");	}string $texEdges[] = {};string $borderEdges[] = {};string $edge;int $sizeE = `size $edges`;int $count = 0;for ($edge in $edges)	{//$edge = $edges[0];	if (`size (convertPoly({$edge}, "tuv"))` > 2)		$texEdges[`size $texEdges`] = $edge;	if (`size (convertPoly ({$edge}, "tf"))` < 2)		$borderEdges[`size $borderEdges`] = $edge;	$count++;	if ($count % 100 == 0 && $showProgress)		{		float $progress = intToFloat($count) / $sizeE * 100;		progressValue $progress;		progressText ($count + " / " + $sizeE);		}	if (progressEsc())		return {};	}if ($showProgress)	progressEnd;return stringArrayCatenate($texEdges, $borderEdges);}