global proc float[] uvsShellBBsize(string $uvs[]){//$uvs = `ls -sl -fl`;float $us[] = {};float $vs[] = {};float $pos[] = {};for ($uv in $uvs)	{//$uv = $uvs[0];	$pos = pointPositionUV($uv);	$us[`size $us`] = $pos[0];	$vs[`size $vs`] = $pos[1];	}float $uSize = maxOf($us) - minOf($us);float $vSize = maxOf($vs) - minOf($vs);return {$uSize, $vSize};}