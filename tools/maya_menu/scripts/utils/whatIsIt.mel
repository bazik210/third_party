global proc string whatIsIt(string $item){//$item = sel1();string $object = stringSingle(`ls -o -hd 1 $item`);if (`size $item` == 0)	return "";string $verts[] = `filterExpand -sm 31 $item`;string $edges[] = `filterExpand -sm 32 $item`;string $faces[] = `filterExpand -sm 34 $item`;string $uvs[] = `filterExpand -sm 35 $item`;if (`size $verts` > 0)	return "v";else if (`size $edges` > 0)	return "e";else if (`size $faces` > 0)	return "f";else if (`size $uvs` > 0)	return "u";else if ($object == $item)	return "o";else	return "";}