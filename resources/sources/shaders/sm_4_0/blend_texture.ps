#include "common.h"

uniform float4 modulate_color;
Texture2D t_frame_color;

float4 main( v2p_TL vin) : SV_TARGET
{
	float3 frame_color = t_frame_color.Sample(s_base, vin.Tex0).rgb;
	return float4( lerp(frame_color, t_base.Sample(s_position, vin.Tex0).rgb*modulate_color.rgb, modulate_color.a), 1);
}










